<!-- MODALE -->
<div id="sessionExpiredModal" class="modal fade bd-example-modal-lg" role="dialog"
     aria-labelledby="myLargeModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">

            <div class="row">
                <div class="col-xs-12 col-md-offset-2 col-md-8 col-md-offset-2">
                    <div class="modal-header">

                        <div class="row">
                            <div class="col-xs-12 ">
                                <h3>{{ 'app.common.sessionExpired' | translate }}</h3>
                                <hr>
                                <p>{{ 'app.common.newAccess' | translate }}</p>
                            </div>
                        </div>

                        <div class="row">
                            <div class="form-group">
                                <div class="col-xs-12 text-center">
                                    <button class="btn bps-btn-primary btn-lg btn-block"
                                            ng-click="goToLogin()">{{ 'app.common.Access' | translate }}</button>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="form-group">
                                <div class="col-xs-12 text-center">
                                    <button class="btn bps-btn-secondary btn-lg btn-block"
                                            ng-click="exit()">{{ 'logout.title' | translate }}</button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
