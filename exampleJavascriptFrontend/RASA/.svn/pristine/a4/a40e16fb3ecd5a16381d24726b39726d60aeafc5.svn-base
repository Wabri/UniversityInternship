<div ng-switch="bvDisableErrorsWrapper">
	<div ng-switch-when="true">
		<div ng-include="'common/directives/datepickerMs/datepickerMs.include.directive.html'" style="padding-bottom: 60px;"></div>
	</div>
	<div ng-switch-default>
		<bv-errors-wrapper bv-warning-text="bvWarningText" bv-error-text="bvErrorText">			
			<div ng-include="'common/directives/datepickerMs/datepickerMs.include.directive.html'" style="padding-bottom:60px"></div>
		</bv-errors-wrapper>
	</div> 
</div>
