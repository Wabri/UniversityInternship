
.header{
  text-align: center;
  font-size: rem(24);
}

div fieldset.standard {

  border-width: 1px;
  border-radius: 0.5rem;
 }

.bv-inline-block{
  display: inline-block;
  margin-left: rem(20);
  margin-right: rem(40);
}



.imageCheck{
  margin-bottom: rem(-10);
  width:40%;
  height:50%;
}

.bv-margin-bottom{
  margin-bottom:rem(20);
}

.bv-margin-bottom{
  margin-top:rem(20);
}

.bv-margin{
  margin-bottom:rem(20);
  margin-top:rem(20);
}



.bv-padding-left{
  padding-left:rem(20);
}

.bv-table-item{
  border:1px solid;
  min-height: rem(40);
  align-items: center;
  margin-bottom: rem(80);
  padding-top: rem(30);
  padding-bottom: rem(20);

}

.bv-center{
  text-align: center;
}

.bv-button-gps{
  width: auto;
  height: rem(50);

}

.bv-button-confirm{
  width: rem(200);
  height: rem(50);


}

.bv-max-height{
  max-height:rem(120);
}


.md-button.md-primary.md-raised.bv-button-gps.bv-custom-button {
    color: rgba(255,255,255,0.87);
    background-color: rgb(0,128,0);
}

.md-button.md-primary.md-raised.bv-button-gps.bv-custom-button:not([disabled]):hover {
    background-color: rgb(0,100,0);
}
