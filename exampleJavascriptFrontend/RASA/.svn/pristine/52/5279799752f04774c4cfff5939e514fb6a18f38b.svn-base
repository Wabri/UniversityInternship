(function () {
  'use strict';
  /**
   * config
   * @type {[type]}
   */
  angular
    .module(APP_NAME)
    .config(config);

  /**
   * [config description]
   * @method config
   * @param  {[type]} $provide [description]
   * @return {[type]}          [description]
   */
  function config($stateProvider) {

    // gli stati
    $stateProvider
      .state('loginpage', {
        url: '/login',
        templateUrl: "loginpage/loginpage.html",
        controller: "loginpageCtrl",
        controllerAs: "vm",
        resolve: {},
        params: {}
      });
  }

}());
